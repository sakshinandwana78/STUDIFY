{
  "expo": {
    "name": "Studify AR",
    "slug": "studify-ar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/tudify (1080 x 1920 px).png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "androidStatusBar": {
      "backgroundColor": "#0B1229",
      "barStyle": "light-content",
      "translucent": false
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sakshi-nandwana2.studifyar",
      "splash": {
        "image": "./assets/tudify (1080 x 1920 px).png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0B1229"
      },
      "splash": {
        "image": "./assets/tudify (1080 x 1920 px).png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.sakshi_nandwana2.studifyar"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "@viro-community/react-viro",
        {
          "android": {
            "xRMode": [
              "AR"
            ]
          }
        }
      ],
      "expo-font"
    ],
    "developmentClient": {
      "silentLaunch": false
    },
    "extra": {
      "DEBUG_AR": true,
      "eas": {
        "projectId": "studify-ar-project"
      }
    }
  }
}
